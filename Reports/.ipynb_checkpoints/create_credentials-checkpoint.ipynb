    "import getpass as gp\n",
    "import pickle\n",
    "\n",
    "user = input(\"Enter user id: \\n\")\n",
    "psw = gp.getpass(\"Enter your password: \\n\")\n",
    "\n",
    "cred_dict = {\n",
    "    \"user\":user,\n",
    "    \"password\":psw\n",
    "}\n",
    "\n",
    "with open(\"credentials\", \"wb\") as f:\n",
    "    pickle.dump(cred_dict,f)"
